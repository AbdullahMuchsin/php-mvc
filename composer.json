{
  "name": "abdullah-muchsin/belajar-php-mvc",
  "type": "project",
  "autoload": {
    "psr-4": {
      "AbdullahMuchsin\\BelajarPhpLoginManagement\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AbdullahMuchsin\\BelajarPhpLoginManagement\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "AbdullahMuchsin",
      "email": "abdullahmuchsin96@gmail.com"
    }
  ],
  "require": {
    "php": ">=8"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.5"
  },
  "scripts": {
    "server": "php -S localhost:8080 -t public",
    "tests": "phpunit --configuration phpunit.xml",
    "tests:all": "phpunit --configuration phpunit.xml --testsuite \"Test All\"",
    "tests:config-database": "phpunit --configuration phpunit.xml --testsuite \"Config Database\""
  }
}
